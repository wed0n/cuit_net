{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "http://localhost:8090/",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "cuit_net",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "wed0n.cuit.net",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "width": 400,
        "height": 300,
        "minWidth": 400,
        "minHeight": 300,
        "title": "CUIT校园网自动连接 By: Wed0n",
        "center": true,
        "fullscreen": false,
        "resizable": true
      }
    ]
  }
}